div.jumbotron {
	background-color: #5c5c5c;
	height: 500px;
	margin-top: 0px;
	background-image: url(<%= asset_path "keira.jpg" %>);
	background-position: -480px -85px ;
	h1 {
		text-align: center;
		margin-top: 200px;
		font-family: 'futura-pt', sans-serif;
		font-size: 70px;
		font-weight: 700;
		text-transform: uppercase;
		text-shadow: 0px 0px 8px black;
	}
	span {
		color: #ff0071;
	}
}